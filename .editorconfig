# http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = tab
indent_size = 4
end_of_line = crlf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
insert_final_newline = true
max_line_length = 0
trim_trailing_whitespace = false

[*.{json,yml}]
indent_style = space
indent_size = 2
